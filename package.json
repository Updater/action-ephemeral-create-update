{
  "name": "action-ephemeral-create-update",
  "version": "1.2.0-0",
  "main": "dist/index.js",
  "repository": "git@github.com:Updater/action-ephemeral-create-update.git",
  "author": "apollorion <joey@apollorion.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0"
  },
  "scripts": {
    "build": "ncc build ./src/index.ts -o dist",
    "prepatch": "yarn build && yarn version --prepatch",
    "preminor": "yarn build && yarn version --preminor",
    "premajor": "yarn build && yarn version --premajor",
    "postversion": "git-branch-is --not 'main' && git push --tags && git push"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.33.3",
    "git-branch-is": "4.0.0",
    "typescript": "^4.5.5"
  }
